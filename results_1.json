              "properties": {
                "category": "Security Best Practices",
                "severity": "MEDIUM"
              }
            },
            {
              "id": "AC_K8S_0051",
              "name": "secretsAsEnvVariables",
              "shortDescription": {
                "text": "Prefer using secrets as files over secrets as environment variables"
              },
              "properties": {
                "category": "Infrastructure Security",
                "severity": "HIGH"
              }
            },
            {
              "id": "AC_K8S_0088",
              "name": "dontConnectDockerSock",
              "shortDescription": {
